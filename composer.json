{
	"name": "thomasmoldovan/dbdbdo",
	"type": "project",
	"description": "Database first, crud and forms, code generator",
	"homepage": "https://www.dbdbdo.com",
	"license": "MIT",
	"minimum-stability": "beta",
	"require": {
		"php": "^7.3||^8.0",
		"codeigniter4/framework": "^4",
		"myth/auth": "^1.0@beta",
		"gajus/dindent": "2.0.*"
	},
	"require-dev": {
		"fakerphp/faker": "^1.9",
		"mikey179/vfsstream": "^1.6",
		"phpunit/phpunit": "^9.1"
	},
	"suggest": {
		"ext-fileinfo": "Improves mime type detection for files"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app",
			"Config\\": "app/Config"
		},
		"exclude-from-classmap": [
			"**/Database/Migrations/**"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Support\\": "tests/_support"
		}
	},
	"scripts": {
		"test": "phpunit"
	},
	"support": {}
}
